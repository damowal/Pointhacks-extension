{
  "name": "pointhacks-autofill",
  "version": "2.0.0",
  "description": "Chrome extension for auto-filling credit card application forms on Australian bank websites",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "node scripts/build.js",
    "build:watch": "node scripts/build.js --watch",
    "zip": "node scripts/build.js && node scripts/zip.js",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write \"src/**/*.{js,css,html}\"",
    "format:check": "prettier --check \"src/**/*.{js,css,html}\"",
    "clean": "rm -rf dist *.zip"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "archiver": "^6.0.1",
    "chokidar": "^3.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/damowal/Pointhacks-extension.git"
  },
  "author": "Point Hacks",
  "license": "MIT"
}
